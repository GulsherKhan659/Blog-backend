{
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/blog",
            "dest": "/index.js"
        },
        {
            "src": "/api/blog/(.*)",
            "dest": "/index.js"
        },
        {
            "src": "/",
            "dest": "/index.js"
        }
    ]
}